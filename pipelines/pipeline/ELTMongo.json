{
	"name": "ELTMongo",
	"properties": {
		"activities": [
			{
				"name": "Extraer Datos De mongo DB",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "@{'garage'}"
					},
					{
						"name": "Destination",
						"value": "indata/DataMongo/"
					}
				],
				"typeProperties": {
					"source": {
						"type": "MongoDbAtlasSource",
						"batchSize": 100
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".csv"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mapComplexValuesToString": false
					}
				},
				"inputs": [
					{
						"referenceName": "generic_mongo_extract",
						"type": "DatasetReference",
						"parameters": {
							"pCollection": "garage"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "generic_csv_output",
						"type": "DatasetReference",
						"parameters": {
							"pFolder": "DataMongo",
							"pFile": "garage"
						}
					}
				]
			}
		],
		"annotations": []
	}
}