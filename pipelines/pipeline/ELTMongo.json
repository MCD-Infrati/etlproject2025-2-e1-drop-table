{
	"name": "ELTMongo",
	"properties": {
		"activities": [
			{
				"name": "MapeoCadaElemento",
				"type": "ForEach",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@pipeline().parameters.collections",
						"type": "Expression"
					},
					"activities": [
						{
							"name": "Extraer Datos Mongo DB",
							"type": "Copy",
							"dependsOn": [],
							"policy": {
								"timeout": "0.12:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [
								{
									"name": "Source",
									"value": "@{item()}"
								},
								{
									"name": "Destination",
									"value": "indata/DataMongo/@{concat(item(),'.csv')}"
								}
							],
							"typeProperties": {
								"source": {
									"type": "MongoDbAtlasSource",
									"batchSize": 100
								},
								"sink": {
									"type": "DelimitedTextSink",
									"storeSettings": {
										"type": "AzureBlobFSWriteSettings",
										"copyBehavior": "FlattenHierarchy"
									},
									"formatSettings": {
										"type": "DelimitedTextWriteSettings",
										"quoteAllText": true,
										"fileExtension": ".txt"
									}
								},
								"enableStaging": false
							},
							"inputs": [
								{
									"referenceName": "generic_mongo_extract",
									"type": "DatasetReference",
									"parameters": {
										"pCollection": "@item()"
									}
								}
							],
							"outputs": [
								{
									"referenceName": "generic_csv_output",
									"type": "DatasetReference",
									"parameters": {
										"pFolder": "'DataMongo'",
										"pFile": {
											"value": "@concat(item(),'.csv')",
											"type": "Expression"
										}
									}
								}
							]
						}
					]
				}
			}
		],
		"parameters": {
			"collections": {
				"type": "array",
				"defaultValue": [
					"garage",
					"pool",
					"bsmt",
					"misc"
				]
			}
		},
		"annotations": []
	}
}